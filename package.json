{
  "name": "@sigureya/rpgtypes",
  "version": "2.3.0",
  "description": "A TypeScript library for RpgMakerMZ-related types.",
  "sideEffects": false,
  "main": "./dist/rpgTypes.cjs.js",
  "module": "./dist/rpgTypes.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/rpgTypes.es.js",
      "require": "./dist/rpgTypes.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "build:mock": "vite build --mode mock",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sigureya/RpgTypes.git"
  },
  "keywords": [
    "RPG Maker",
    "RPGMakerMZ",
    "types",
    "typescript"
  ],
  "author": "Max Siguren",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^22.15.29",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.0",
    "eslint-plugin-functional": "^9.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-sort-exports": "^0.9.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.2",
    "fast-glob": "^3.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": {
    "url": "https://github.com/Sigureya/RpgTypes/issues"
  },
  "homepage": "https://github.com/Sigureya/RpgTypes#readme",
  "dependencies": {
    "ajv": "^8.17.1",
    "jsonschema": "^1.5.0"
  }
}
