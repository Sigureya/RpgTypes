{
  "name": "@sigureya/rpgtypes",
  "version": "2.0.0",
  "description": "A TypeScript library for RpgMakerMZ-related types.",
  "sideEffects": false,
  "main": "./packages/index.ts",
  "exports": {
    ".": {
      "types": "./packages/schema/dist/index.d.ts",
      "import": "./packages/schema/dist/schema.es.js",
      "require": "./packages/schema/dist/schema.cjs.js"
    },
    "./namedItemSource": {
      "types": "./packages/namedItemSource/dist/index.d.ts",
      "import": "./packages/namedItemSource/dist/namedItemSource.es.js",
      "require": "./packages/namedItemSource/dist/namedItemSource.cjs.js"
    }
  },
  "files": [
    "packages/**/dist/**/*",
    "packages/**/package.json"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sigureya/RpgTypes.git"
  },
  "keywords": [
    "RPG Maker",
    "RPGMakerMZ",
    "types",
    "typescript"
  ],
  "author": "Max Siguren",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.21.0",
    "typescript": "^5.0.0",
    "vite": "^6.2.6",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": {
    "url": "https://github.com/Sigureya/RpgTypes/issues"
  },
  "homepage": "https://github.com/Sigureya/RpgTypes#readme"
}
