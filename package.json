{
  "sideEffects": false,
  "name": "@sigureya/rpgtypes",
  "version": "2.0.0",
  "description": "A TypeScript library for RpgMakerMZ-related types.",
  "main": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./packages/schema/dist/schema.cjs.js",
      "types": "./packages/schema/dist/index.d.ts",
      "import": "./packages/schema/dist/rpgtypes.es.js"
    }
  },
  "files": [
    "packages/**/dist/**/*"
  ],
  "scripts": {
    "build": "npx pnpm -r run build",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sigureya/RpgTypes.git"
  },
  "keywords": [
    "RPG Maker",
    "RPGMakerMZ",
    "types",
    "typescript"
  ],
  "author": "Max Siguren",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.21.0",
    "typescript": "^5.0.0",
    "vite": "^6.2.5",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^2.1.9"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": {
    "url": "https://github.com/Sigureya/RpgTypes/issues"
  },
  "homepage": "https://github.com/Sigureya/RpgTypes#readme"
}
