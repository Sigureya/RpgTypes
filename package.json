{
  "name": "@sigureya/rpgtypes",
  "version": "2.0.0",
  "description": "A TypeScript library for RpgMakerMZ-related types.",
  "sideEffects": false,
  "main": "./dist/libs/index.ts",
  "exports": {
    ".": {
      "types": "./dist/libs/index.d.ts",
      "import": "./dist/libs/rpgTypes.es.js",
      "require": "./dist/libs/rpgTypes.cjs.js"
    },
    "./mock": {
      "types": "./dist/mock/mock/index.d.ts",
      "import": "./dist/mock/rpgMocks.es.js",
      "require": "./dist/mock/rpgMocks.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "build:mock": "vite build --mode mock",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sigureya/RpgTypes.git"
  },
  "keywords": [
    "RPG Maker",
    "RPGMakerMZ",
    "types",
    "typescript"
  ],
  "author": "Max Siguren",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^9.25.0",
    "typescript": "^5.0.0",
    "vite": "^6.2.6",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.1.1",
    "eslint-plugin-functional": "^9.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bugs": {
    "url": "https://github.com/Sigureya/RpgTypes/issues"
  },
  "homepage": "https://github.com/Sigureya/RpgTypes#readme"
}
